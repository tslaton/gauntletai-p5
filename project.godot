[display]

window/size/viewport_width=3456
window/size/viewport_height=2234

[dotnet]

project/assembly_name="Red Horizon"

[editor_plugins]

enabled=PackedStringArray("res://addons/terrain_3d/plugin.cfg")

[rendering]

textures/vram_compression/import_etc2_astc=true
